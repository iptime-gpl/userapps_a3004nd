#
# Program options...
#
# ARCHFLAGS     Defines the default architecture build options.
# OPTIM         Defines the common compiler optimization/debugging options
#               for all architectures.
# OPTIONS       Defines other compile-time options (currently only -DDEBUG
#               for extra debug info)
#

ALL_CFLAGS	=	-I.. -D_CUPS_SOURCE $(CFLAGS) $(SSLFLAGS) \
			 -D_THREAD_SAFE -D_REENTRANT $(ONDEMANDFLAGS) $(OPTIONS)
ALL_CXXFLAGS	=	-I.. -D_CUPS_SOURCE $(CXXFLAGS) $(SSLFLAGS) \
			 -D_THREAD_SAFE -D_REENTRANT $(ONDEMANDFLAGS) $(OPTIONS)
ARCHFLAGS	=	
ARFLAGS		=	crvs
BACKLIBS	=	
BUILDDIRS	=	filter backend berkeley cgi-bin monitor notifier ppdc scheduler systemv conf data desktop locale man doc examples templates
CFLAGS		=	 -DUSE_EFM -I/home/mt7621/userapps/nasapp/include -fPIC
COMMONLIBS	=	-lpthread -lm -lcrypt -L/home/mt7621/userapps/nasapp/lib -liconv -lrt -lz
CXXFLAGS	=	 -DUSE_EFM -I/home/mt7621/userapps/nasapp/include
CXXLIBS		=	
DBUS_NOTIFIER	=	
DBUS_NOTIFIERLIBS =	
DNSSD_BACKEND	=	
DSOFLAGS	=	-L../cups 
DSOLIBS		=	 $(COMMONLIBS)
DNSSDLIBS	=	
IPPFIND_BIN	=	
IPPFIND_MAN	=	
LDFLAGS		=	-L../cgi-bin -L../cups -L../filter -L../ppdc \
			-L../scheduler  \
			-L/home/mt7621/userapps/nasapp/lib  -fPIE -pie $(OPTIM)
LINKCUPS	=	../cups/libcups.a $(LIBGSSAPI) $(DNSSDLIBS) $(LIBZ)
LINKCUPSIMAGE	=	../filter/libcupsimage.a
LIBS		=	$(LINKCUPS) $(COMMONLIBS)
ONDEMANDFLAGS	=	
ONDEMANDLIBS	=	
OPTIM		=	-Wall -Wno-format-y2k -Wunused -Os -g -Wsign-conversion -Wno-tautological-compare -D_GNU_SOURCE
OPTIONS		=
PAMLIBS		=	
SERVERLIBS	=	 
SSLFLAGS	=	
SSLLIBS		=	
UNITTESTS	=	


#
# Directories...
#
# The first section uses the GNU names (which are *extremely*
# difficult to find in a makefile because they are lowercase...)
# We have to define these first because autoconf uses ${prefix}
# and ${exec_prefix} for most of the other directories...
#
# The "datarootdir" variable may not get defined if you are using
# a version of autoconf prior to 2.60.
#
# This is immediately followed by definition in ALL CAPS for the
# needed directories...
#

bindir		=	/usr/bin
datadir		=	/usr/share
datarootdir	=	/usr/share
exec_prefix	=	/usr
includedir	=	/usr/include
infodir		=	${datarootdir}/info
libdir		=	/usr/lib64
libexecdir	=	${exec_prefix}/libexec
localstatedir	=	/var
mandir		=	/usr/share/man
oldincludedir	=	/usr/include
prefix		=	/
privateinclude	=	
sbindir		=	/usr/sbin
sharedstatedir	=	/usr/com
srcdir		=	.
sysconfdir	=	/etc
top_srcdir	=	.

BUILDROOT	=	$(DSTROOT)

AMANDIR		=	$(BUILDROOT)/usr/share/man
BINDIR		=	$(BUILDROOT)/usr/bin
BUNDLEDIR	=	
CACHEDIR	=	$(BUILDROOT)/var/cache/cups
DATADIR		=	$(BUILDROOT)/usr/share/cups
DOCDIR		=	$(BUILDROOT)/usr/share/doc/cups
ICONDIR		=	/usr/share/icons
INCLUDEDIR	=	$(BUILDROOT)$(includedir)
LIBDIR		=	$(BUILDROOT)$(libdir)
LOCALEDIR	= 	$(BUILDROOT)/usr/share/locale
LOGDIR		= 	$(BUILDROOT)/var/log/cups
MANDIR		=	$(BUILDROOT)/usr/share/man
MENUDIR		=	/usr/share/applications
PMANDIR		=	$(BUILDROOT)/usr/share/man
PRIVATEINCLUDE	=	$(BUILDROOT)
RCLEVELS	=	2 3 5
RCSTART		=	81
RCSTOP		=	36
REQUESTS	= 	$(BUILDROOT)/var/spool/cups
SBINDIR		=	$(BUILDROOT)/usr/sbin
SERVERBIN	= 	$(BUILDROOT)/usr/lib/cups
SERVERROOT	= 	$(BUILDROOT)/etc/cups
STATEDIR	=	$(BUILDROOT)/var/run/cups

MAN1EXT		=	1.gz
MAN5EXT		=	5.gz
MAN7EXT		=	7.gz
MAN8EXT		=	8.gz
MAN8DIR		=	8

